{
  "name": "inoby",
  "version": "1.0.0",
  "description": "inoby wp theme",
  "scripts": {
    "build": "rm -rf build && webpack --mode=production",
    "build:dev": "rm -rf build && webpack --mode=development",
    "watch": "webpack --watch --mode=development",
    "build:pot": "wp-pot --dest-file 'rootscope.pot' --domain 'inoby' --src '**/*.php'"
  },
  "devDependencies": {
    "@prettier/plugin-php": "^0.21.0",
    "@types/jquery": "^3.5.13",
    "@types/jqueryui": "^1.12.16",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.1",
    "autoprefixer": "~9.7.4",
    "css-loader": "~3.4.2",
    "mini-css-extract-plugin": "^2.5.3",
    "postcss-loader": "~3.0.0",
    "precss": "~4.0.0",
    "prettier": "^3.0.3",
    "sass": "~1.23.7",
    "sass-loader": "~8.0.2",
    "style-loader": "~1.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "url-loader": "~4.1.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-remove-empty-scripts": "^0.7.3",
    "wp-pot-cli": "~1.5.0"
  },
  "dependencies": {
    "bootstrap": "4.5.3",
    "glightbox": "^3.1.0",
    "popper.js": "~1.16.0"
  }
}
